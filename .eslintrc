{
  "env": {
    "browser": true,
    "jquery": true,
    "node": true
  },
  "globals": {
    "API": true
  },
  "rules": {
    "semi": [2, "always"],
    "camelcase": 2,
    "eqeqeq": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "max-len": [
      2,
      {
        "code": 300,
        "ignoreComments": true
      }
    ],
    "new-cap": 2,
    "quotes": [
      2,
      "single"
    ],
    "strict": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "no-eq-null": 2
  },
  "parserOptions": {
    "ecmaVersion": 8
  }
}